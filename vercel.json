{
  "version": 2,
  "name": "igbot2025",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  },
  "regions": ["iad1"],
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 300
    }
  }
}
