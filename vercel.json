{
  "version": 2,
  "framework": null,
  "installCommand": "pip install -r api/requirements.txt",
  "buildCommand": "echo 'Python serverless - no build step'",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
