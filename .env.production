# Production Environment Variables for Vercel
# DO NOT COMMIT THIS FILE - Add these to Vercel Dashboard

# Environment
ENVIRONMENT=production

# Instagram Accounts (Format: username:password:2fa_secret,username2:password2:2fa_secret2)
IG_ACCOUNTS=

# Dashboard Authentication
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=changeme

# Anti-Ban Configuration
ENABLE_HUMAN_SIMULATION=true
WARMUP_PERIOD_DAYS=14
MAX_FOLLOWS_PER_DAY=200
MAX_LIKES_PER_DAY=500
MAX_COMMENTS_PER_DAY=50
SHADOWBAN_DETECTION=true

# Scheduler Configuration
TIMEZONE=America/Los_Angeles
PEAK_HOURS_START=10
PEAK_HOURS_END=21
ACTIONS_PER_HOUR_MIN=30
ACTIONS_PER_HOUR_MAX=60

# Proxy Configuration (Optional but recommended)
PROXY_API_KEY=
PROXY_POOL_SIZE=100
PROXY_ROTATION_INTERVAL=300
PROXY_PROVIDERS=

# Database URLs (For external services)
DATABASE_URL=
MONGODB_URL=
REDIS_URL=

# Content Generation (Optional)
OPENAI_API_KEY=
AUTO_REMIX_REELS=false
FYP_OPTIMIZATION=true
HASHTAG_RESEARCH=true

# Monitoring (Optional)
SENTRY_DSN=
LOG_LEVEL=INFO
ENABLE_METRICS=true

# Emergency Controls
KILL_SWITCH_ENABLED=false
AUTO_PAUSE_ON_ANOMALY=true
MAX_BAN_RISK_SCORE=75
